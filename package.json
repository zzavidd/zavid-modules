{
  "name": "zavid-modules",
  "version": "4.1.3",
  "description": "A library containing reusable code modules for projects developed by Zavid Egbue.",
  "main": "./_dist/build/main.js",
  "types": "./_dist/index.d.ts",
  "scripts": {
    "prebuild": "rm -rf build _dist && tsc --project tsconfig.json",
    "build": "webpack --mode production --config-name production",
    "check": "tsc --noEmit --project ./tsconfig.json",
    "dev": "webpack-dev-server --config-name development",
    "release": "npm run build && np --no-release-draft",
    "release:only": "np --no-release-draft",
    "test": "ts-mocha --project test/tsconfig.test.json --recursive --no-timeout test/**/*.ts",
    "lint": "eslint --ignore-path .gitignore \"**/*.js\"",
    "prettify": "prettier --ignore-path .gitignore --write \"**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zzavidd/zavid-modules.git"
  },
  "author": "Zavid Egbue",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zzavidd/zavid-modules/issues"
  },
  "homepage": "https://github.com/zzavidd/zavid-modules#readme",
  "dependencies": {
    "@aarnila/react-instagram-embed": "^2.0.2",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-instagram-embed": "^1.5.0",
    "react-textarea-autosize": "^7.1.2",
    "react-toastify": "^5.5.0",
    "react-twitter-embed": "^3.0.3",
    "webpack": "^4.41.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.11",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.0",
    "dotenv-webpack": "^5.1.0",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-mocha": "^7.0.0",
    "eslint-plugin-react": "^7.19.0",
    "html-webpack-plugin": "^4.3.0",
    "mocha": "^7.0.1",
    "node-sass": "^4.13.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^1.1.2",
    "style-loader": "^1.1.2",
    "ts-loader": "^8.0.11",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.0.0",
